{
    "compilerOptions": {
        "baseUrl": "./",
        "strictNullChecks": true,
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "preserve",
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "ES6",
    },
    "exclude": ["node_modules"]
}